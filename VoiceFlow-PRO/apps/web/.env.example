# AIML API Configuration
# Get your API key from https://aimlapi.com
VITE_AIML_API_KEY=your_api_key_here
VITE_AIML_BASE_URL=https://api.aimlapi.com/v1

# Application Configuration
VITE_APP_NAME=VoiceFlow Pro
VITE_APP_VERSION=1.0.0

# Supabase Configuration (Optional - for database features)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Stripe Configuration (Optional - for payment features)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here

# Feature Flags
VITE_ENABLE_REAL_TIME_STREAMING=false
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_ANALYTICS=false

